<div class="container">
  <div class="row">
    <div class="col">
      <h4 class="my-5">商品一覧</h4>
      
        <table class="table">
          
          <thead>
            <tr>
              <th>商品ID</th>
              <th>商品名</th>
              <th>税抜価格</th>
              <th>ステータス</th>
            </tr>
          </thead>
          <tbody>
          <% @items.each do |item| %>
            <tr>
              <td><%= item.id %></td>
              <td><%= link_to item.name, admin_items_path(item) %></td>
              <td><%= item.price %></td>
              <td></td>
            </tr>
          <% end %>
          </tbody>
        </table>
        
    </div>
  </div>
</div>